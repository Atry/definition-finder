{
  "roots": [ "src/", "tests/" ],
  "builtinLinters": "all",
  "namespaceAliases": {
    "HHAST": "Facebook\\HHAST\\Linters"
   },
  "overrides": [
    {
      "patterns": [ "tests/data/*" ],
      "disabledLinters": [
        "HHAST\\CamelCasedMethodsUnderscoredFunctionsLinter",
