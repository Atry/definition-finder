{
  "name": "facebook/definition-finder",
  "description": "Find definitions in PHP or Hack files. Useful for autoloaders.",
  "keywords": ["hack", "definitions", "hhvm", "autoload"],
  "homepage": "https://github.com/hhvm/definitions-finder",
  "require": {
    "hhvm": "~3.12"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.1",
    "hhvm/systemlib-extractor": "^0.1.0",
    "91carriage/phpunit-hhi": "~5.1"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "files": [ "autoload_files.php" ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  }
}
